{
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.tsx(doc)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "destination": "docs",
      "recurse": true
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,
      "default": {
        "outputSourceFiles": true
      }
    }
  }
